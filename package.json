{
  "name": "webpack_and_configuration",
  "version": "1.0.0",
  "description": "webpack and configuration",
  "main": "index.js",
  "jest": {
    "coveragePathIgnorePatterns": [
      "/index.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development  node ./server",
    "start:dev": "NODE_ENV=development webpack-dev-server --open",
    "start:prod": "NODE_ENV=production webpack-dev-server --open",
    "test": "jest ./tests/ --coverage",
    "test:coverage": "jest --watch --coverage",
    "build:dev": "NODE_ENV=development webpack --mode development",
    "build:prod": "NODE_ENV=production webpack --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ControlX/NetflixRoulette.git"
  },
  "keywords": [
    "webpack_and_configuration"
  ],
  "author": "Abhishek Verma",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ControlX/NetflixRoulette/issues"
  },
  "homepage": "https://github.com/ControlX/NetflixRoulette#readme",
  "dependencies": {
    "@babel/plugin-syntax-jsx": "^7.12.1",
    "cross-env": "^7.0.2",
    "formik": "^2.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.13.0",
    "react-redux": "^7.2.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^4.2.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.3.1",
    "react-test-renderer": "^17.0.1",
    "redux-mock-store": "^1.5.4",
    "style-loader": "^1.2.1",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^4.0.2",
    "webpack-dev-server": "^3.11.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-hot-server-middleware": "^0.6.1",
    "webpack-merge": "^5.4.0",
    "webpack-node-externals": "^2.5.2"
  }
}
